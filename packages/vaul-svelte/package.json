{
	"name": "vaul-svelte",
	"version": "0.3.2",
	"license": "MIT",
	"repository": "github:huntabyte/vaul-svelte",
	"scripts": {
		"build": "pnpm package",
		"dev": "pnpm watch",
		"dev:svelte": "vite dev",
		"package": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"test": "vitest",
		"watch": "svelte-package --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.0.0-next.1"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@testing-library/dom": "^9.3.3",
		"@testing-library/svelte": "^4.0.5",
		"@testing-library/user-event": "^14.5.2",
		"@types/node": "^20.10.5",
		"autoprefixer": "^10.4.16",
		"clsx": "^2.1.0",
		"jsdom": "^23.0.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.0.0",
		"vitest-dom": "^0.1.1"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"bits-ui": "https://pkg.pr.new/huntabyte/bits-ui/bits-ui@8cd05f0"
	}
}
